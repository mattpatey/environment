Config { font = "xft:Consolas:size=9:antialias=false"
       , bgColor = "#111111"
       , fgColor = "#cccccc"
       , position = BottomW L 95
       , lowerOnStart = True
       , commands = [ Run Weather "EDDB" ["-t"," <tempC>C","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %H:%M" "date" 10
                    , Run StdinReader
	            , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
	            , Run Com "srv" [] "server_status" 50
	            , Run Com "current_branch" ["/home/mlp/workspace/ableton/web"] "current_branch" 50
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% } Currently on <fc=#00ff00>%current_branch%</fc>, server PID: <fc=#ffff00>%server_status%</fc> { %eth0% | %cpu% | %memory% * %swap%    <fc=#ee9a00>%date%</fc> | %EDDB%"
       }
